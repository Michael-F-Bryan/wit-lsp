{
    "release-type": "rust",
    "packages": {
        "crates/wit-compiler": {
            "component": "wit-compiler"
        },
        "crates/wit-cli": {
            "component": "wit-cli"
        },
        "crates/wit-language-server": {
            "component": "wit-language-server"
        },
        "tree-sitter-wit": {
            "component": "tree-sitter-wit",
            "extra-files": [
                {
                    "type": "json",
                    "path": "tree-sitter-wit/package.json",
                    "jsonpath": "$.version"
                },
                {
                    "type": "toml",
                    "path": "tree-sitter-wit/pyproject.toml",
                    "jsonpath": "$.project.version"
                },
                "tree-sitter-wit/Makefile"
            ]
        },
        "plugins/vscode": {
            "component": "vscode-plugin",
            "release-type": "node"
        }
    },
    "plugins": [
        "cargo-workspace"
    ],
    "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
